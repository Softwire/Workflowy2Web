{
  "manifest_version": 2,

  "name": "Workflowy Prototype Converter",
  "description": "This extension converts a workflowy page to an HTML prototype.",
  "version": "1.0",

  "permissions": [
    "tabs",
    "https://workflowy.com/s/*",
    "declarativeContent"
  ],
  "background":{
    "persistent":false,
    "page":"background.html"
  },
  "page_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_title": "Workflowy2Web"
  },
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [ "https://workflowy.com/*" ],
      "js": [
        "lib/require.min.js",
        "lib/jquery-2.1.4.min.js",
        "lib/requireContent.js",
        "lib/requireConfig.js",
        "lib/jszip.min.js",
        "lib/jszip-utils.min.js",
        "lib/FileSaver.min.js",
        "lib/async.min.js",
        "background.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "resources/*",
    "lib/*",
    "src/*"
  ]
}